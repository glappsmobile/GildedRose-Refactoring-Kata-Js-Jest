{
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "ignorePatterns": ["*.test.ts"],
    "rules": {
      "import/prefer-default-export": "off",
      "import/no-unresolved": "off",
      "import/extensions": "off"
    }
}
